<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="QSelectAvalonia.Pages.HomePage">
	<Panel Classes="page">
		<StackPanel Orientation="Vertical" HorizontalAlignment="Center">
			<Grid>
				<Image Source="resm:QSelectAvalonia.Assets.QSelectLogo.png" HorizontalAlignment="Center" Stretch="None"/>
				<TextBlock x:Name="SplashTextBlock" Classes="Splash" Text="[]" HorizontalAlignment="Right" VerticalAlignment="Bottom"/>
			</Grid>
		</StackPanel>
	</Panel>
</UserControl>
