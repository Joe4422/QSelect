<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="clr-namespace:QSelectAvalonia.ViewModels;assembly=QSelectAvalonia"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:pages="clr-namespace:QSelectAvalonia.Pages"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="QSelectAvalonia.Views.MainWindow"
        Icon="/Assets/avalonia-logo.ico"
        Title="QSelectAvalonia">

    <Design.DataContext>
        <vm:MainWindowViewModel/>
    </Design.DataContext>

  <Grid>
    <TabControl Name="Sidebar">
      <TabItem Header="Mods"><pages:ModPage/></TabItem>
      <TabItem Header="Engines"><pages:EnginePage/></TabItem>
      <TabItem Header="Downloads"><pages:DownloadsPage/></TabItem>
      <TabItem Header="Settings"><pages:SettingsPage/></TabItem>
    </TabControl>
  </Grid>

</Window>
